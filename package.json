  "description": "stc-ui",
  "main": "index.js",
  "scripts": {
    "dll-dev": "cross-env NODE_ENV=development webpack --colors  --progress --config build/site/dll.config.js",
    "dll-prod": "cross-env NODE_ENV=production webpack --colors  --progress --config build/site/dll.config.js",
    "server": "cross-env NODE_ENV=development webpack-dev-server --inline --display-error-details --config build/site/dev.config.js",
    "prod": "cross-env NODE_ENV=production webpack --colors --hide-modules --config build/site/prod.config.js",
    "dev": "npm run svg && npm run dll-dev && npm run server",
    "build": "cross-env NODE_ENV=production node build/site/build.js",
    "linux-build": "NODE_ENV=production node build/site/build.js",
    "svg": "node build/site/svg.config.js",
    "build:file": "node build/build.entry.js",
    "test": "cross-env CI_ENV=/dev/ BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "lint": "cross-env NODE_ENV=lint eslint --fix --debug --ext .js,.vue src",
    "fundebug": "cross-env NODE_ENV=production webpack --colors  --progress --config build/site/fundebug.config.js"
  },
